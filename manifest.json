{
  "manifest_version": 2,

  "name": "Socket Puppet",
  "description": "This extension sniffs websockets.",
  "version": "0.1",

  "permissions": ["tabs","<all_urls>"],
  
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["injector.js"],
	  "run_at": "document_start"
    }
  ],
  
  "web_accessible_resources": [
    "socketPuppet.js"
  ]
}